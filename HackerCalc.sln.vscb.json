{
  "vscb_version": "1.0",
  "description": "This is an example solution vscb config file.",
  "homepage_url": "https://github.com/PaulHuizer/VsCommandBuddy/",
  "author": "Paul Huizer",
  "verbose": true,
  "macros": {
    "distDir": "$(SolutionDir)\\dist",
    "testdataDir": "$(distDir)\\test"
  },
  "commands": [
    {
        "cmdname": "compilecoco",
        "title": "Compile Coco.atg",
        "description": "Compile Coco.atg",
        "cwd": "$(SolutionDir)\\HackerCalc.Parser",
        "filename": "$(SolutionDir)\\HackerCalc.Parser\\Coco\\PreBuild.cmd",
        "arguments": "$(SolutionDir)\\HackerCalc.Parser $(Configuration)",
        "async": false,
        "key1": "2",
        "mod1": "CTRL SHIFT"
    }
  ]
}